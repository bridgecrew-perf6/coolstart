{
  "name": "static",
  "version": "0.1.0",
  "description": "An 11ty starter deployed on Netlify",
  "main": "index.js",
  "scripts": {
    "start": "npm run tokens && npx gulp && concurrently \"npx gulp watch\" \"npx eleventy --serve\"",
    "production": "npm run tokens && NODE_ENV=production npx gulp && NODE_ENV=production eleventy",
    "tokens": "generate-properties --out src/css/partials/config.css",
    "lint": "eslint --ext .js"
  },
  "author": "frzrbox",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "autoprefixer": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-prettier": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-postcss": "^9.0.0",
    "html-minifier": "^4.0.0",
    "husky": "^4.3.6",
    "netlify-plugin-image-optim": "^0.4.0",
    "netlify-plugin-inline-critical-css": "^1.1.3",
    "node": "^15.4.0",
    "postcss": "^8.2.1",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.35.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
